package com.bernice.demo;

import java.util.Arrays;
import java.util.Collection;
import java.util.List;

public class IntegerList {
	public static void main(String[] args) {
		List<Integer> names=Arrays.asList(3, -64, 77, 23, 300, -12);
		
		Collection<Integer> numbers = null;
		long negNumbers=numbers.stream().filter((n)->n>0).count();

		System.out.println("Total numbers with negative integers " + negNumbers);
	}
}
